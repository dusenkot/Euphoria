<body>
    <%- include('../partials/search.ejs')  %>
    <div class="section_hero">
        <div class="section_hero-bg">
            <img src="/img/back.jpg" alt="">
            <div class="section_hero-bg-overlay"></div>
            <div class="section_hero-bg-scroll-overlay"></div>
        </div>
    </div>
    <div class="container">
        <%- include('../partials/header.ejs')  %>
        <main>
            <%- body  %>
        </main>
    </div>
    <div class="fa-3x">
        <i class="ri-cloud-line cloud fa-shake fa-solid"></i>
    </div>
    <div class="block">
        <div class="animate__animated  animate__bounceOutUp  rope_cloud">
            <div class="img_cloud">
                <ul class="header__list">
                    <li><a href="/">Discover</a></li>
                    <li><a href="/">Activities</a></li>
                    <li><a href="/">Journal</a></li>
